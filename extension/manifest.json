{
  "manifest_version": 2,

  "name": "WikiClassify",
  "description": "Extends the functionality of WikiClassify.com to your browser.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
